#nexus

BEGIN Taxa;
DIMENSIONS ntax=5;
TAXLABELS
[1] 'A'
[2] 'B'
[3] 'C'
[4] 'D'
[5] 'outgroup'
;
END; [Taxa]

BEGIN Splits;
DIMENSIONS ntax=5 nsplits=8;
FORMAT labels=no weights=yes confidences=no intervals=no;
PROPERTIES fit=-1.0 compatible;
CYCLE 1 2 3 4 5;
MATRIX
[1, size=1] 	 0.5 	  2 3 4 5,
[2, size=1] 	 0.25 	  1 3 4 5,
[3, size=1] 	 1.0 	  1 2 4 5,
[4, size=3] 	 1.5 	  1 2 3 5,
[5, size=1] 	 1.0 	  1 2 3 4,
[6, size=1] 	 0.25 	  1 2 3,
[7, size=1] 	 0.75 	  1 2,
[8, size=1] 	 0.25 	  2 3,
;
END; [Splits]
